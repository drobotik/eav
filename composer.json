{
    "name": "drobotik/eav",
    "description": "multi domain EAV package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drobotik\\Eav\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aleksandr Drobotik"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/validation": "^9.0|^10.0",
        "illuminate/translation": "^9.0|^10.0",
        "ext-pdo": "*",
        "league/csv": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "phpunit/phpunit": "9.6.5",
        "doctrine/dbal": "^3.6",
        "doctrine/migrations": "4.0.x-dev",
        "symfony/yaml": "6.3.x-dev",
        "doctrine/orm": "2.14.1",
        "symfony/cache": "6.3.x-dev"
    }
}
