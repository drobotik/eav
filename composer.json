{
    "name": "kuperwood/eav",
    "description": "multi domain EAV package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kuperwood\\Eav\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alex Kuperwood"
        }
    ],
    "minimum-stability": "dev",
