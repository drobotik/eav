        "doctrine/dbal": "^3.6",
        "doctrine/migrations": "4.0.x-dev",
        "symfony/yaml": "6.3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "fakerphp/faker": "^1.21"
    }
}
